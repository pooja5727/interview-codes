#include <stdio.h>

int main(void) {
	// your code goes here
	int n,rem,sum,temp;
	//printf("enter n");
	scanf("%d",&n);
	temp=n;
	while(n>0)
	{
	    rem=n%10;
	    sum=sum*10+rem;
	    n=n/10;
	}
	n=temp;
	if(sum==n)
	{
	    printf("palindrome");
	}
	else
	{
	    printf("not palindrome");
	}
	return 0;
}

